    "modern",
    "responsive"
  ],
  "author": " <buraksenturk25@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meftunca/reflex/issues"
  },
  "homepage": "meftunca.github.io/reflex",
  "dependencies": {
    "@popperjs/core": "^2.9.0",
    "color2k": "^1.2.4"
  },
  "peerDependencies": {
    "@emotion/styled": "*",
    "@emotion/react": "*",
    "react": "*",
    "react-dom": "*",
    "react-table": "*",
    "dayjs": "*"
  },
  "devDependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-table": "^7.0.28",
    "@types/resize-observer-browser": "^0.1.5",
    "enzyme": "^3.11.0",
    "fs-extra": "^9.1.0",
    "jest-diff": "^26.6.2",
    "microbundle": "^0.13.0",
    "react": "^17.0.1",
    "react-docgen-typescript": "^1.21.0",
    "react-dom": "^17.0.1",
    "react-table": "^7.6.3",
    "react-test-renderer": "^17.0.1",
    "tslib": "2.1.0",
    "typescript": "^4.2.2",
    "dayjs": "^1.10.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
