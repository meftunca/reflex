    "ui kit",
    "material",
    "paper design",
    "design",
    "ui",
    "modern",
    "responsive"
  ],
  "author": " <buraksenturk25@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meftunca/reflex/issues"
  },
  "homepage": "meftunca.github.io/reflex",
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "color2k": "^1.1.1"
  },
  "peerDependencies": {
    "@emotion/styled": "*",
    "@emotion/react": "*",
    "react": "*",
    "react-dom": "*",
    "react-table": "*"
  },
  "devDependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-table": "^7.0.26",
    "enzyme": "^3.11.0",
    "fs-extra": "^9.0.1",
    "jest-diff": "^26.6.2",
    "microbundle": "^0.13.0",
    "react": "^17.0.1",
    "react-docgen-typescript": "^1.20.5",
    "react-dom": "^17.0.1",
    "react-table": "^7.6.2",
    "react-test-renderer": "^17.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "tslib": "2.0.3",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
